{
  "private": true,
  "name": "guerilla-bikes",
  "description": "",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn build:css && remix build",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --minify",
    "dev": "cross-env NODE_ENV=development concurrently \"yarn dev:css\" \"run-p dev:*\"",
    "postinstall": "remix setup cloudflare-pages",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "wrangler pages dev ./public --watch ./build",
    "start": "npm run dev:wrangler"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@popmotion/popcorn": "^0.4.4",
    "@react-three/fiber": "^7.0.26",
    "@remix-run/cloudflare-pages": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "add": "^2.0.6",
    "framer-motion": "^6.2.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "three": "^0.137.5",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.1.3",
    "@svgr/cli": "^6.2.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "0.13.14",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.1.2",
    "wrangler": "alpha"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}
